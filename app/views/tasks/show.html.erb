<p id="notice"><%= notice %></p>

<p>
  <strong>タスク名:</strong>
  <%= @task.name %>
</p>

<p>
  <strong>詳しい内容:</strong>
  <%= @task.description %>
</p>

<p>
  <strong>ステータス:</strong>
  <%= @task.status %>
</p>

<p>
  <strong>優先度:</strong>
  <%= @task.priority %>
</p>

<p>
  <strong>終了期限:</strong>
  <%= @task.deadline.strftime("%Y-%m-%d") %>
</p>

<p>
  <strong>登録日:</strong>
  <%= @task.created_at.strftime("%Y-%m-%d") %>
</p>
<p>
  <strong>ラベル名:</strong>
  <% @task.labels.each do |label| %>
    <%= label.name %>
  <% end %>
</p>
<%= link_to '編集', edit_task_path(@task) %> |
<%= link_to '戻る', tasks_path %>
